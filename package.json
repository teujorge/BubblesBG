{
    "name": "bubblesbg",
    "version": "0.9.0",
    "description": "A smooth yet bubbly background for your web app.",
    "main": "src/cjs/index.js",
    "module": "src/esm/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rmdir /s /q dist && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/teujorge/BubblesBG.git"
    },
    "keywords": [
        "balls",
        "floating",
        "bubbles",
        "background"
    ],
    "author": "Matheus Jorge",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/teujorge/BubblesBG/issues"
    },
    "homepage": "https://github.com/teujorge/BubblesBG#readme",
    "type": "module",
    "devDependencies": {
        "@emotion/css": "^11.10.6",
        "@types/node": "^18.14.6",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/typescript": "^2.0.0",
        "typescript": "^4.9.5",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
